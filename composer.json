{
    "name": "vedavith/liberator-fork",
    "description": "A proxy for circumventing PHP access modifier restrictions.",
    "keywords": ["access", "modifier", "object", "proxy", "private", "protected", "reflection"],
    "homepage": "https://github.com/vedavith/liberator-fork",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/vedavith/liberator-fork/issues",
        "source": "https://github.com/vedavith/liberator-fork"
    },
    "replace": {
        "eloquent/liberator": "self.version"
    },
    "authors": [
        {
            "name": "Erin Millard",
            "email": "ezzatron@gmail.com",
            "homepage": "http://ezzatron.com/"
        },
        {
            "name": "Vedavith Ravula",
            "email": "veda_ravula@outlook.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "vedavith/pops-fork": "^6.0"
    },
    "require-dev": {
        "eloquent/code-style": "^2",
        "errors/exceptions": "^0.2",
        "friendsofphp/php-cs-fixer": "^3",
        "phpunit/phpunit": "^11 || ^12"
    },
    "autoload": {
        "psr-4": {
            "Eloquent\\Liberator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eloquent\\Liberator\\": ["test/src"]
        },
        "files": [
            "test/src/SeriousBusiness.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-main": "3.0.x-dev"
        }
    }
}
